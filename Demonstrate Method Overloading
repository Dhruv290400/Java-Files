class Q7 {
    int add(int a, int b) {
        return a + b;
    }

    double add(double a, double b) {
        return a + b;
    }

    String add(String a, String b) {
        return a + b;
    }

    public static void main(String[] args) {
        Q7 obj = new Q7();
        System.out.println("Sum of integers: " + obj.add(10, 20));
        System.out.println("Sum of doubles: " + obj.add(5.5, 6.5));
        System.out.println("Concatenation: " + obj.add("Hello ", "World"));
    }
}
